{
    "name": "laraerp/core",
    "description": "ERP brasileiro de cÃ³digo fonte aberto escrito sob o Laravel Framework",
    "keywords": ["erp", "openerp", "erp php", "laravel"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=7.2.0",
        "ext-gd": "*",
        "ext-curl": "*",
        "laravel/framework": "^6.2",
        "fideloper/proxy": "^4.0",
        "laravel/tinker": "^2.0",
        "jansenfelipe/cidades-gratis": "^2.0",
        "jansenfelipe/cep-gratis": "^3.0",
        "jansenfelipe/cnpj-gratis": "^3.0",
        "jansenfelipe/utils": "^2.0",
        "jansenfelipe/cpf-gratis": "^3.0",
        "jansenfelipe/cfop": "^1.0",
        "jansenfelipe/ncm": "^1.0",
        "jansenfelipe/nfephp-serialize": "^3.11",
        "nfephp-org/sped-nfe": "^5.0",
        "barryvdh/laravel-debugbar": "^3.2.8",
        "laraerp/ordination": "^2.0"
    },
    "require-dev": {
        "orchestra/testbench": "^4.5",
        "phpunit/phpunit": "^8.0"
    },
    "repositories": [{
            "type": "path",
            "url": "../../cidades-gratis"
        },
        {
            "type": "path",
            "url": "../../cpf-gratis"
        },
        {
            "type": "path",
            "url": "../../cnpj-gratis"
        },
        {
            "type": "path",
            "url": "../../cep-gratis"
        },
        {
            "type": "path",
            "url": "../../utils"
        },
        {
            "type": "path",
            "url": "../../nfephp-serialize"
        },
        {
            "type": "path",
            "url": "../../ordination"
        }
    ],
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "autoload": {
        "classmap": [
            "database/seeds",
            "database/migrations"
        ],
        "psr-4": {
            "Laraerp\\": "src"
        }
    }
}