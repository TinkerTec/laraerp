{
    "name": "laraerp/laraerp",
    "description": "ERP brasileiro de cÃ³digo fonte aberto escrito sob o Laravel Framework",
    "keywords": [
        "laraerp",
        "laravel"
    ],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=7.2.0",
        "ext-gd": "*",
        "ext-curl": "*",
        "laravel/framework": "^6.2",
        "fideloper/proxy": "^4.0",
        "laravel/tinker": "^2.0",
        "laracasts/flash": "^3.0",
        "jansenfelipe/cidades-gratis": "^2.0",
        "jansenfelipe/cep-gratis": "^3.0",
        "jansenfelipe/cnpj-gratis": "^3.0",
        "jansenfelipe/utils": "^2.0",
        "jansenfelipe/cpf-gratis": "^3.0",
        "jansenfelipe/cfop": "^1.0",
        "jansenfelipe/ncm": "^1.0",
        "jansenfelipe/nfephp-serialize": "^3.11",
        "nfephp-org/sped-nfe": "^5.0",
        "barryvdh/laravel-debugbar": "^3.2.8",
        "laraerp/ordination": "^2.0"
    },
    "require-dev": {
        "facade/ignition": "^1.4",
        "fzaninotto/faker": "^1.4",
        "mockery/mockery": "^1.0",
        "nunomaduro/collision": "^3.0",
        "phpunit/phpunit": "^8.0",
        "phpspec/phpspec": "~6.1",
        "symfony/dom-crawler": "~5.0",
        "symfony/css-selector": "~5.0",
        "composer/composer": "^1.9"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "repositories": [{
            "type": "path",
            "url": "../cidades-gratis"
        },
        {
            "type": "path",
            "url": "../cpf-gratis"
        },
        {
            "type": "path",
            "url": "../cnpj-gratis"
        },
        {
            "type": "path",
            "url": "../cep-gratis"
        },
        {
            "type": "path",
            "url": "../utils"
        },
        {
            "type": "path",
            "url": "../nfephp-serialize"
        },
        {
            "type": "path",
            "url": "../ordination"
        }
    ],
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "psr-4": {
            "App\\": "app/",
            "Laraerp\\": [
                "core/src",
                "template/src"
            ]
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    }
}